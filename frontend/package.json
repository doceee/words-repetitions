{
    "name": "app",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": "^20.0.0",
        "npm": "^10.0.0"
    },
    "scripts": {
        "dev": "vite --port 8080",
        "build": "run-p type-check build-only",
        "preview": "vite preview --port 4173",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit",
        "eslint": "eslint . --ext .vue,.ts --fix --cache --cache-location ./node_modules/.cache/.eslintcache",
        "prettier": "prettier . --write --ignore-path=.eslintignore",
        "cy:run": "cypress run --browser chrome --headless --config-file cypress.config.ts",
        "test": "start-server-and-test dev http-get://localhost:8080 cy:run",
        "test:e2e": "cd ../backend && NODE_ENV=test node dist/src/main.js & pid=$! && npx wait-on http-get://localhost:3000/api && cd ../frontend && npm run dev && npm run cy:run && kill -9 $pid"
    },
    "dependencies": {
        "@heroicons/vue": "2.0.18",
        "@tailwindcss/aspect-ratio": "0.4.2",
        "@tailwindcss/forms": "0.5.4",
        "@vuelidate/core": "2.0.3",
        "@vuelidate/validators": "2.0.4",
        "axios": "1.6.2",
        "dayjs": "1.11.13",
        "pinia": "2.1.4",
        "sass": "1.64.1",
        "sass-loader": "13.3.2",
        "start-server-and-test": "^2.0.12",
        "vue": "3.3.4",
        "vue-router": "4.2.4",
        "vue-toastification": "2.0.0-rc.5"
    },
    "devDependencies": {
        "@babel/eslint-parser": "7.22.9",
        "@rushstack/eslint-patch": "1.3.2",
        "@types/node": "18.16.14",
        "@vitejs/plugin-vue": "4.2.3",
        "@vue/eslint-config-prettier": "9.0.0",
        "@vue/eslint-config-typescript": "13.0.0",
        "@vue/tsconfig": "0.4.0",
        "autoprefixer": "10.4.14",
        "cypress": "^14.5.0",
        "eslint": "8.57.0",
        "eslint-plugin-vue": "9.27.0",
        "npm-run-all": "4.1.5",
        "postcss": "8.4.31",
        "prettier": "3.0.1",
        "prettier-plugin-tailwindcss": "0.5.10",
        "tailwindcss": "3.3.3",
        "typescript": "5.1.6",
        "vite": "4.4.7",
        "vue-tsc": "1.8.0"
    }
}
